{
  "name": "tyuryaga-server",
  "version": "1.0.0",
  "description": "Backend server for Tyuryaga game",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed:bosses": "node src/scripts/seedBosses.js",
    "seed:items": "node src/scripts/seedItems.js",
    "seed:works": "node src/scripts/seedWorks.js",
    "restore:energy": "node src/scripts/restoreUserEnergy.js",
    "update:energy": "node src/scripts/updateUserEnergy.js",
    "test:rewards": "node src/scripts/testBossRewards.js",
    "test:shop": "node src/scripts/testShopAndInventory.js"
  },
  "keywords": [
    "game",
    "multiplayer",
    "socket.io"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.0",
    "socket.io": "^4.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "eslint": "^9.37.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.6.2"
  }
}
